        <TextView
            android:id="@+id/txtsamosa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:foregroundGravity="center"
            android:gravity="center"
            android:text="SAMOSA"
            android:textColor="#2D2B28"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="210dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/cucurbadak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/popia"
        app:layout_constraintVertical_bias="0.1">

        <ImageView
            android:id="@+id/ivcucurbadak"
            android:layout_width="109dp"
            android:layout_height="84dp"
            app:srcCompat="@drawable/cucurbadak"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="126dp" />

        <TextView
            android:id="@+id/txtcucurbadak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:foregroundGravity="center"
            android:gravity="center"
            android:text="CUCUR BADAK"
            android:textColor="#2D2B28"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="210dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/kuihkasturi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/popia"
        app:layout_constraintVertical_bias="0.1">

        <ImageView
            android:id="@+id/ivkasturi"
            android:layout_width="109dp"
            android:layout_height="84dp"
            app:srcCompat="@drawable/kuihkasturi"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="126dp" />

        <TextView
            android:id="@+id/txtkasturi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:foregroundGravity="center"
            android:gravity="center"
            android:text="KUIH KASTURI"
            android:textColor="#2D2B28"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="210dp" />

    </LinearLayout>

    <ImageView
        android:id="@+id/logout"
        android:layout_width="60dp"
        android:layout_height="67dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.923"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.969"
        app:srcCompat="@drawable/loggg" />

    <ImageView
        android:id="@+id/order"
        android:layout_width="76dp"
        android:layout_height="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.969"
        app:srcCompat="@drawable/foodod" />

    <ImageView
        android:id="@+id/homepage"
        android:layout_width="77dp"
        android:layout_height="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        app:srcCompat="@drawable/homemem" />
</androidx.constraintlayout.widget.ConstraintLayout>